// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "typescript.check.workspaceVersion": false,
    "files.exclude": {
        ".*": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "lib/": false,
        "typings": true,
        "yarn.lock": true,
        "node_modules": false,
        "package.json": true,
        "tsconfig.json": true,
        "tslint.json": true,
        "*.log*": true
    }
}